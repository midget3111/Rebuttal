{
  "name": "rebuttal",
  "version": "1.0.0",
  "description": "",
  "main": "app.js",
  "scripts": {
    "server": "nodejs index.js",
    "client": "electron --enable-transparent-visuals .",
    "test-client": "electron  --ignore-certificate-errors --enable-transparent-visuals --disable-gpu .",
    "test": "node test/index.js",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "postinstall": "electron-builder install-app-deps"
  },
  "author": {
    "name": "trigg",
    "email": "triggerhapp@gmail.com"
  },
  "license": "GPL-3.0-or-later",
  "dependencies": {
    "bcrypt": "^5.0.1",
    "better-sqlite3": "^7.4.0",
    "body-parser": "^1.19.0",
    "buffer-image-size": "^0.6.4",
    "express": "^4.17.1",
    "gravatar": "^1.8.1",
    "mysql": "^2.18.1",
    "socket.io": "^4.0.2",
    "uuid": "^8.3.2",
    "ws": "^7.4.5"
  },
  "devDependencies": {
    "electron": "^12.0.7",
    "electron-builder": "22.10.4",
    "node-gyp": "^8.0.0"
  },
  "build": {
    "appId": "com.electron.rebuttal.app",
    "productName": "Rebuttal Client",
    "copyright": "Copyright Â©2021 Nathan Howard",
    "publish": null,
    "dmg": {
      "contents": [
        {
          "x": 110,
          "y": 150
        },
        {
          "x": 240,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "win": {
      "target": [
        "nsis"
      ]
    },
    "linux": {
      "target": [
        "AppImage",
        "snap",
        "deb",
        "rpm",
        "pacman",
        "tar.gz"
      ]
    }
  }
}